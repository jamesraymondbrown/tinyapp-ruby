<p id="notice"><%= notice %></p>

<h1>Urls</h1>

<table>
  <thead>
    <tr>
      <th>Short url</th>
      <th>Long url</th>
      <th>Description</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @urls.each do |url| %>
      <tr>
        <td><%= url.short_url %></td>
        <td><%= url.long_url %></td>
        <td><%= url.description %></td>
        <td><%= url.user_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


